{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "indent": ["error", 4],
        "max-len": ["error", 240],
        "arrow-parens": 0,
        "camelcase": "off",
        "object-curly-spacing": "off"
    }
}
